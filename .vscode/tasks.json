    },
    {
      "label": "GitHub: Open Repo (Simple Browser)",
      "command": "${input:githubRepoBrowser}",
      "problemMatcher": []
    }
  ],
  "inputs": [
    {
      "id": "githubLoginBrowser",
      "type": "command",
      "command": "simpleBrowser.show",
      "args": ["https://github.com/login"]
    },
    {
      "id": "githubRepoBrowser",
      "type": "command",
      "command": "simpleBrowser.show",
      "args": ["https://github.com/Lllewellla/subscription-tracker"]
    }
  ]
}
